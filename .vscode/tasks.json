{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Typecheck",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"typecheck"
			],
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "Lint",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"lint",
				"--",
				"--no-cache"
			],
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}